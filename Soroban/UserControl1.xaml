            <Ellipse x:Name="bes" HorizontalAlignment="Left" Height="100" Margin="750,50,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FFD17575"/>
            <Ellipse x:Name="dort" HorizontalAlignment="Left" Height="100" Margin="750,615,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
            <Ellipse x:Name="uc" HorizontalAlignment="Left" Height="100" Margin="750,510,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
            <Ellipse x:Name="iki" HorizontalAlignment="Left" Height="100" Margin="750,405,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
