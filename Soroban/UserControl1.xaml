                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform X="0" Y="0"/>
                    </TransformGroup>
                </Ellipse.RenderTransform>
            </Ellipse>
            <Ellipse HorizontalAlignment="Left" Height="100" Margin="150,615,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
            <Ellipse HorizontalAlignment="Left" Height="100" Margin="150,510,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1" RenderTransformOrigin="0.377,-0.089"/>
            <Ellipse HorizontalAlignment="Left" Height="100" Margin="150,405,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
            <Ellipse HorizontalAlignment="Left" Height="100" Margin="150,300,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" Fill="#FF75B6D1"/>
